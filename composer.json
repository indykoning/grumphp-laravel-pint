{
    "name": "indykoning/grumphp-laravel-pint",
    "description": "A Laravel Pint task for GrumPHP.",
    "type": "plugin",
    "keywords": [
        "laravel",
        "grumphp",
        "quality",
        "phpcsfixer",
        "pint",
        "linter"
    ],
    "homepage": "https://github.com/Indykoning/grumphp-laravel-pint",
    "license": "MIT",
    "authors": [
        {
            "name": "James Hemery",
            "email": "james@yieldstudio.fr",
            "homepage": "https://yieldstudio.fr",
            "role": "Developer"
        },
        {
            "name": "Indy Koning",
            "homepage": "https://indykoning.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "phpro/grumphp": "*"
    },
    "autoload": {
        "psr-4": {
            "Indykoning\\GrumPHPLaravelPint\\": "src"
        }
    },
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {}
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
